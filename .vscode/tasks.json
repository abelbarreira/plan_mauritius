{
  "version": "2.0.0",
  "tasks": [
    // Start Docker.
    {
      "label": "Start Docker",
      "type": "shell",
      "command": "./start_docker",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": {
        "owner": "c",
        "fileLocation": [
          "absolute"
        ],
        "pattern": {
          "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      }
    },
  ]
}